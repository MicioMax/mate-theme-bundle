{
  "name": "contao-themes-net/mate-theme-bundle",
  "type":"contao-bundle",
  "description": "mate theme bundle for Contao 4",
  "keywords":["contao","theme","template"],
  "homepage": "https://contao-themes.net",
  "license": "pdir contao theme licence",
  "authors": [
    {
      "name": "Webdesign fÃ¼r Dresden",
      "homepage": "https://pdir.de",
      "email" : "develop@pdir.de",
      "role" : "Developer"
    },
    {
      "name": "Kostenlose Contao Themes",
      "homepage": "https://contao-themes.net",
      "email" : "develop@pdir.de",
      "role" : "Supplier"
    }
  ],
  "support" : {
    "email" : "develop@pdir.de",
    "issues" : "https://github.com/contao-themes-net/mate-theme-bundle/issues",
    "source" : "https://github.com/contao-themes-net/mate-theme-bundle"
  },
  "require": {
    "php": ">=7.0.0",
    "contao/core-bundle": "^4.4",
    "pdir/contao-theme-helper-bundle": "dev-master"
  },
  "conflict": {
    "contao/core": "*"
  },
  "autoload": {
    "psr-4": {
      "ContaoThemesNet\\MateThemeBundle\\": "src/"
    },
    "classmap": [
      "src/ContaoThemesNet/MateThemeBundle/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/ContaoThemesNet/MateThemeBundle/Resources/contao/config/",
      "src/ContaoThemesNet/MateThemeBundle/Resources/contao/dca/",
      "src/ContaoThemesNet/MateThemeBundle/Resources/contao/languages/",
      "src/ContaoThemesNet/MateThemeBundle/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "ContaoThemesNet\\MateThemeBundle\\ContaoManager\\Plugin"
  },
  "config": {
    "files": "files"
  }
}
